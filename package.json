{
  "name": "gifs",
  "version": "0.0.1",
  "description": "Giftopia",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "lint": "eslint . && stylelint src/stylesheets/*.css"
  },
  "author": "Brian Jacobel",
  "license": "MIT",
  "private": false,
  "repository": "http://github.com/bjacobel/gifs",
  "dependencies": {
    "auth0-lock": "^10.7.3",
    "aws-sdk": "^2.7.19",
    "classnames": "^2.2.3",
    "fuse.js": "^2.6.1",
    "query-string": "^4.2.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-infinite": "^0.10.0",
    "react-redux": "^5.0.1",
    "redux": "^3.2.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.3.13",
    "core-js": "^2.3.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.25.0",
    "imports-loader": "^0.7.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "react-transform-hmr": "^1.0.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.0",
    "stylelint": "^7.7.0",
    "stylelint-config-standard": "^15.0.1",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "jest": {
    "mocksPattern": "(?:[\\/]|^)mocks[\\/]",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleNameMapper": {
      "aws-sdk/global": "<rootDir>/mocks/aws-sdk/global.js",
      "aws-sdk/clients/s3": "<rootDir>/mocks/aws-sdk/clients/s3.js",
      "aws-sdk/clients/dynamodb": "<rootDir>/mocks/aws-sdk/clients/dynamodb.js",
      "\\.(jpe?g|png|gif)$": "<rootDir>/mocks/fileMock.js",
      "\\.css$": "<rootDir>/mocks/styleMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "tests/.*/.*-test.js$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
